{
    "name": "kazimolmez/google-play-scraper",
    "type": "library",
    "description": "A PHP scraper to get app data from Google Play",
    "keywords": ["google play", "scraper"],
    "homepage": "https://github.com/kazimolmez/google-play-scraper",
    "license": "MIT",
    "require": {
        "fabpot/goutte": "~3.0"
    },
    "authors": [{
        "name": "Kazım Ölmez",
        "email": "kazim-olmez@yandex.com"
    }],
    "autoload": {
        "psr-4": {
            "Kazim\\GooglePlayScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kazim\\GooglePlayScraper\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "mockery/mockery": "^1.0"
    }
